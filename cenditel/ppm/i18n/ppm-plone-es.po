msgid "Approved by City Council"
msgstr "Aprobado por el Alcalde"

#: ../profiles/default/workflows/proposal_workflow/definition.xml
msgid "Approved by City Manager"
msgstr "Aprobado por el Director ejecutivo"

#: ../profiles/default/workflows/proposal_workflow/definition.xml
